<html>
<head>
    <meta charset="utf-8">
</head>
<body>
<form method="post" action="filter">
    <input type="text" name="dc"/>
    <button type="submit">Filter by code</button>
</form>
<div>
    <table>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Job Title</th>
            <th>Employment date</th>
            <th>Department code</th>
        </tr>
{{#employeeList}}
        <tr>
            <td>{{name}}</td>
            <td>{{surname}}</td>
            <td>{{jobTitle}}</td>
            <td>{{employmentDate}}</td>
            <td>{{departmentCode}}</td>
        </tr>
{{/employeeList}}
    </table>
</div>
</body>
</html>